{
  "projectType": "application",
  "sourceRoot": "apps/fury/src",
  "prefix": "fury",
  "targets":
  {
    "build":
    {
      "executor": "@angular-devkit/build-angular:browser",
      "outputs": ["{options.outputPath}"],
      "options":
      {
        "outputPath": "dist/apps/fury",
        "index": "apps/fury/src/index.html",
        "main": "apps/fury/src/main.ts",
        "tsConfig": "apps/fury/src/tsconfig.app.json",
        "polyfills": "apps/fury/src/polyfills.ts",
        "stylePreprocessorOptions": {
          "includePaths": [
            "apps/fury/src/styles",
            "node_modules"
         ]
        }
      },
      "assets": [
        "apps/fury/src/assets",
        "apps/fury/src/favicon.ico",
        "apps/fury/src/_redirects",
        "sapps/fury/rc/.htaccess"
      ],
      "styles": [
        "apps/fury/src/styles.scss"
      ],
      "scripts": [],
      "baseHref": "/",
      "allowedCommonJsDependencies": [
        "simplebar",
        "highlight.js",
        "faker",
        "showdown",
        "core-js/modules/es.string.match",
        "core-js/modules/es.array.for-each",
        "core-js/modules/es.string.replace",
        "core-js/modules/es.regexp.exec",
        "core-js/modules/es.function.name",
        "core-js/modules/es.string.name",
        "core-js/modules/es.array.reduce",
        "core-js/modules/web.dom-collections.iterator",
        "core-js/modules/es.string.iterator",
        "core-js/modules/es.weak-map",
        "core-js/modules/es.parse-int",
        "core-js/modules/es.object.to-string",
        "core-js/modules/es.object.assign",
        "core-js/modules/es.array.iterator",
        "core-js/modules/es.array.filter",
        "core-js/modules/web.dom-collections.for-each",
        "dom-set",
        "dom-plane",
        "angular-calendar/date-adapters/date-fns",
        "calendar-utils/date-adapters/date-fns",
        "@mattlewis92/dom-autoscroller",
        "angular-calendar/date-adapters/moment",
        "ngx-quill",
        "lodash",
        "chart.js"
      ],
      "vendorChunk": true,
      "extractLicenses": false,
      "buildOptimizer": false,
      "sourceMap": true,
      "optimization": false,
      "namedChunks": true
    },
    "configurations":
    {
      "production":
      {
        "budgets":
        [
          {
            "type": "initial",
            "maximumWarning": "500kb",
            "maximumError": "1mb"
          },
          {
            "type": "anyComponentStyle",
            "maximumWarning": "25kb"
          }
        ],
        "optimization": true,
        "sourceMap": false,
        "namedChunks": false,
        "extractLicenses": true,
        "vendorChunk": false,
        "buildOptimizer": true,
        "fileReplacements":
        [
          {
            "replace": "apps/fury/src/environments/environment.ts",
            "with": "apps/fury/src/environments/environment.prod.ts"
          }
        ],
        "outputHashing": "all"
      },
      "development":
      {
        "buildOptimizer": false,
        "optimization": false,
        "vendorChunk": true,
        "extractLicenses": false,
        "sourceMap": true,
        "namedChunks": true
      },
      "defaultConfiguration": "production"
    },
    "serve":
    {
      "executor": "@angular-devkit/build-angular:dev-server",
      "configurations":
      {
        "production":
        {
          "browserTarget": "fury:build:production"
        },
        "development":
        {
          "browserTarget": "fury:build:development"
        }
      },
      "defaultConfiguration": "development"
    },
    "extract-i18n":
    {
      "executor": "@angular-devkit/build-angular:extract-i18n",
      "options":
      {
        "browserTarget": "fury:build"
      }
    },
    "lint":
    {
      "executor": "@nrwl/linter:eslint",
      "options":
      {
        "lintFilePatterns": ["apps/web/src/**/*.ts", "apps/web/src/**/*.html"]
      }
    },
    "test":
    {
      "executor": "@nrwl/jest:jest",
      "outputs": ["coverage/apps/fury"],
      "options":
      {
        "main": "apps/fury/src/test.ts",
        "karmaConfig": "./karma.conf.js",
        "polyfills": "apps/fury/src/polyfills.ts",
        "tsConfig": "apps/fury/src/tsconfig.spec.json",
        "scripts": [],
        "styles": [
          "apps/fury/src/styles.scss"
        ],
        "assets": [
          "apps/fury/src/assets",
          "apps/fury/src/favicon.ico",
          "apps/fury/src/manifest.json",
          "apps/fury/src/manifest.json"
        ]
      }
    }
  },
  "tags": []
}
